mRectOne.pasRect( rectLeft, rectBottom, rectRight, rectTop );
Rect moveRect = new Rect( rectLeft, rectBottom, rectRight, rectTop ); 
paint.setColor( Color.RED );
canvas.drawRect( moveRect, paint);
Rect coverRect = new Rect( pastLeft, pastBottom, pastRight, pastTop );
